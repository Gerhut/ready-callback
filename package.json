    "koa",
    "ready",
    "async"
  ],
  "main": "index",
  "dependencies": {
    "debug": "~2.1.1",
    "ready": "~0.1.1"
  },
  "devDependencies": {
    "coveralls": "2",
    "istanbul": "0",
    "jshint": "*",
    "koa": "~0.16.0",
    "mocha": "1",
    "should": "4",
    "spy": "~0.1.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popomore/koa-ready"
  },
  "homepage": "https://github.com/popomore/koa-ready",
  "author": "popomore <sakura9515@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "jshint .",
    "test": "_mocha -R spec -t 20000",
    "cov": "jshint . && node --harmony node_modules/istanbul/lib/cli.js cover node_modules/mocha/bin/_mocha -- -R spec -t 20000",
